%% Example BibTeX Entries for a CV or Academic Profile

%% Journal Article (In Review)
@article{Key2024_inreview,
  author    = {{\textbf{LastName, FirstName}} and Coauthor, A.B. and Coauthor, C.D.},
  title     = {Title of the Manuscript Under Review},
  year      = {2024},
  keywords  = {inreview},
  journal   = {Journal Name}
  % optional fields: volume = {}, number = {}, pages = {}, doi = {}, url = {}
}

%% Journal Article (In Preparation)
@article{Key2024_inprep,
  author    = {Coauthor, E.F. and Coauthor, G.H. and {\textbf{LastName, FirstName}} and Coauthor, I.J.},
  title     = {Title of the Manuscript In Preparation},
  year      = {2024},
  keywords  = {inprep},
  journal   = {Another Journal Name}
  % add optional fields as needed
}

%% Conference Presentation/Abstract (Misc)
@misc{Key2024_ConferenceA,
  author       = {{\textbf{LastName, FirstName}} and Coauthor, K.L. and Coauthor, M.N.},
  title        = {Title of the Conference Presentation or Abstract},
  howpublished = {Conference Name, Location},
  month        = oct,   % use month abbreviations: jan, feb, mar, apr, may, jun, jul, aug, sep, oct, nov, dec
  year         = {2024},
  keywords     = {conference}
  % optional: add a url field if available, or a note field for more detail
}

